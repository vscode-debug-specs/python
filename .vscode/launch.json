{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Module test",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
			"pythonPath": "${config:python.pythonPath}",
			"module": "unittest",
			"args": [
				"test_bubble_sort.TestBubbleSort.test_bubble_sort"
			],
			"cwd": "${workspaceRoot}",
			"env": {},
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
				"BreakOnSystemExitZero",
				"RedirectOutput"
			]
		},
		{
			"name": "launch Python",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceFolder}/bubble_sorter.py",
			"args": [
				"4",
				"3",
				"2",
				"1"
			],
			"cwd": "${workspaceFolder}",
			"env": {},
			"envFile": "${workspaceFolder}/.env",
			"debugOptions": [
				"RedirectOutput"
			]
		},
		{
			"name": "Integrated Terminal/Console",
			"type": "python",
			"request": "launch",
			"stopOnEntry": true,
			"pythonPath": "${config:python.pythonPath}",
			"program": "${workspaceFolder}/bubble_sorter.py",
			"cwd": "",
			"console": "integratedTerminal",
			"env": {},
			"args": [
				"4",
				"3",
				"2",
				"1",
			],
			"envFile": "${workspaceRoot}/.env",
			"debugOptions": [
			]
		},
		{
			"name": "Attach (Remote Debug)",
			"type": "python",
			"request": "attach",
			"localRoot": "${workspaceRoot}",
			"remoteRoot": "/home/nnyn/vscode-debug-specs/python",
			"port": 3333,
			"secret": "my_secret",
			"host": "192.168.1.24"
		}
	],
	"compounds": []
}